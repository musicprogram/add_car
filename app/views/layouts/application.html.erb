<!DOCTYPE html>
<html>
<head>
  <title>AddCar</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  

  <nav id="nav">
          
    <div id="page_nav">
        
            <%= link_to "Home", root_path%>
            <%= link_to "Shop", products_path%>
            <%= link_to "About", page_about_path%>
            <%= link_to "FaQs", page_faqs_path%>
            <%= link_to "Contact", page_contact_path%>
            
    </div>
        
    <div id="sign_in">
        <% if user_signed_in? %>
                  <%= link_to "sign_out", destroy_user_session_path, method: :delete, data: { confirm: 'Are you sure?' } %>
                <%else%>    
                  <%= link_to "sign_in", new_user_session_path %>
                  <%= link_to "sign_up", new_user_registration_path %>
        <%end%> 
    </div>       
  </nav>       
       

<div id="main_wrap">


    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>
    
    
    <%= yield %> 


</div>
     
      

</body>
</html>
